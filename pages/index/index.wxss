.home{
  position: fixed;
  height: 100%;
  width: 100%;
  background: #f6f9ff;
}
.bg-img{
  position: absolute;
  bottom: 0;
  right: 0;
}
.container{
  position: absolute;
  width: calc(100% - 60px);
  padding: 50px 30px;
  font-size: 14px;
  color: #666;
}
.text{
  line-height: 50px;
}
.link{
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 60px 0px;
}
.link-list{
  width: 50%;
  text-align: center;
 
}
.link-img{
  width: 100%;
  max-height: 40px;
}