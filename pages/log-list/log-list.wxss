/* pages/logs-list/logs-list.wxss */
.write-btn{
  position: fixed;
  z-index: 1;
  top: 10px;
  right: 10px;
  width: 40px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 50%;
  background: #eee;
  color: #666;
  font-size: 12px;
}
.title{
  text-align: center;
  line-height: 40px;
  color: #333;
}
.log-box{
  padding-top: 10px;
}
.log-list{
  border-bottom: 1px solid #eee;
}
.log-one{
  display: flex;
  padding: 5px 10px;
}
.img{
  width: 40px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 12px;
  color: #fff;
  border-radius: 50%;
}
.content-box{
  flex: 1;
  padding-left: 10px;
}
.name{
  font-size: 14px;
  color: rgb(26, 60, 134);
}
.content{
  font-size: 14px;
  color: #666;
}
.time{
  font-size: 12px;
  color: rgb(143, 142, 142);
}